module analysis/structs
language c18

test declare type [[
struct Person
{
    char name[50];
    int citNo;
    float salary;
};
]] analysis succeeds

test declare inline (single) [[
struct Person {
	char name[50];
    int citNo;
    float salary;
} p;
]] analysis succeeds

test declare inline (multiple) [[
struct Person
{
    char name[50];
    int citNo;
    float salary;
} person1, person2, p[20];
]] analysis succeeds
